{
  "name": "lunicorn",
  "private": true,
  "version": "0.0.1",
  "description": "lunicorn js consumer",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src --presets babel-preset-es2015 --out-dir lib",
    "deploy": "npm run build && npm run package && npm run uglify",
    "package": "rm -rf dist/* && browserify lib/index.js -o dist/lunicorn-$npm_package_version.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "uglify": "uglifyjs  dist/lunicorn-$npm_package_version.js -o dist/lunicorn-$npm_package_version.min.js -c -m -r Lunicorn"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/zivity/lunijs.git#ae268f9"
  },
  "author": "",
  "bugs": {
    "url": "https://github.com/zivity/lunijs/issues"
  },
  "homepage": "https://github.com/zivity/lunijs#readme",
  "dependencies": {
    "pym.js": "^0.4.5"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-preset-es2015": "^6.5.0",
    "browserify": "^13.0.0",
    "uglify-js": "^2.6.2"
  }
}
